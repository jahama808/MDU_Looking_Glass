After=network.target

[Service]
Type=oneshot
User=%u
WorkingDirectory=/home/jahama/Documents/home/jahama/MDU/theNewOutageLookingGlass
ExecStart=/home/jahama/Documents/home/jahama/MDU/theNewOutageLookingGlass/auto-process-data.sh
